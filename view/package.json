{
	"name": "clash",
	"private": true,
	"version": "0.1.0",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"postbuild": "bunx svelte-sitemap -d https://clash.nwrenger.dev -i '404.html' -c weekly -r",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
